{
	"name": "bibliography-manager",
	"version": "1.1.0",
	"description": "A bibliography and source management plugin for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production --metafile=meta.json",
		"build:split": "tsc -noEmit -skipLibCheck && node esbuild.config.split.mjs production",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"bibliography",
		"citations",
		"bibtex",
		"sources",
		"academic"
	],
	"author": "Stefan Danzl",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.8",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^20.19.23",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^30.2.0",
		"obsidian": "latest",
		"rollup": "^4.52.5",
		"rollup-plugin-filesize": "^10.0.0",
		"ts-jest": "^29.4.5",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "^5.4.21"
	},
	"dependencies": {
		"@citation-js/core": "^0.7.18",
		"@citation-js/plugin-bibtex": "^0.7.18",
		"@citation-js/plugin-doi": "^0.7.18",
		"@citation-js/plugin-hayagriva": "^0.1.2",
		"@citation-js/plugin-isbn": "^0.4.0",
		"yaml": "^2.8.1"
	}
}
